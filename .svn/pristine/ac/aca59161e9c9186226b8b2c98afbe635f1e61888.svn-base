              $("#con-alert").html('- Falta capturar el mensaje');
            }
        });

        $(".navbar-brand").click(function(){
          $("html, body").animate({ scrollTop: 0 }, "slow");
          return false;
        });
        //-----------------------------------------
        //            AJAX FUNCTIONS
        //-----------------------------------------
        function ajaxSendContacto(){
          $.ajax({
          data: { /*"_token": "<?php echo e(csrf_token()); ?>",*/ nombre: $("#con-nombre").val(),
                  correo: $("#con-correo").val(), mensaje: $("#con-mensaje").val() },
          type: "POST",
          url: "<?php echo e(url('/submitComments')); ?>",
          beforeSend: function() {
            $("#con-process").addClass('animateElm');
            $("#con-process").html("Enviando comentarios...");
          }
          })
          .done(function(data){
            console.log('ajaxSendContacto(ok)');
            console.log(data);
            $("#con-process").removeClass('animateElm');
            $("#con-process").html("Comentarios enviados");
            $("#con-alert").html("");
            $('#formContacto')[0].reset();
          })
          .fail(function(data){
            console.log(data)
            $("#con-process").removeClass('animateElm');
            $("#con-process").html("");
            $("#con-alert").html("Error, favor de reintentarlo");
            console.log('ajaxSendContacto("falló")');
          });
        }
      });
    </script>

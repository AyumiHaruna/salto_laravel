#mainContainer{
  margin-top: 10%;
}
.clickeable{
  cursor:pointer;
  background-color: #7f7f7f;
  width: auto;
}
.bottomElements{
  align-items:  flex-end;
}
#perfilPhoto{
  height: 150px;
  width: auto;
  border-radius: 50%;
}
.editIcon{
  position:absolute;
  right: 20px;
  top: 5px;
  display:none;
}
.transparentDiv{
  background-color: rgb(0,0,0, 0.5);
  z-index: 8;
  position: fixed;
  height: 100vh;
  width: 100%;
  top:0;
  display:none;
}
.alert-div{
  background-color: #fff;
  z-index: 9;
  position: fixed;
  top: 15%;
  padding: 10px;
  -webkit-box-shadow: 0px 2px 5px 0px rgba(122,122,122,1);
  -moz-box-shadow: 0px 2px 5px 0px rgba(122,122,122,1);
  box-shadow: 0px 2px 5px 0px rgba(122,122,122,1);
  display:none;
}
.hiddenForm{
  background-color: #fff;
  z-index: 9;
  position: fixed;
  top: 35%;
  padding: 20px;
  display:none;
}
.closeBtn{
  background: #fff;
  color: red;
}

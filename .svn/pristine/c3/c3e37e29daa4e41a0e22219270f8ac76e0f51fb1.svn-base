/* carousel */
.carouselHeader{
  font-size: 3.5vw;
  font-family: "Raleway Bold";
  color: #fff;
  z-index: 10;
  padding-top: 5%;
  padding-left: 5%;
  margin-bottom: -17%;
}
.carousel{
  height: 80vh;
  overflow: visible;
  /* max-width: 104%;
  overflow-x: hidden; */
}
.carouselDiv{
  height: 84vh;
  background-size: cover;
}
.slick-slide {
    height: 104% !important;
}
.carouselText{
  font-size: 3vw;
  font-family: "Raleway";
  display: inline;
  padding-top: 20%;
  padding-left: 5%;
  padding-right: 5%;
  font-weight: bold;
}
.slick-dots {
    position: absolute;
    bottom: 0px !important;
    display: block;
    width: 100%;
    list-style: none;
    text-align: center;
    height: 100px;
    background-color: rgb(0,0,0, 0.5);
    padding-top: 35px !important;
}
.slick-dots li button:before {
    font-size: 62px!important;
}
.slick-dots li {
    margin: 0px 1% !important;
}
.slick-dots li button:before {
    color: white !important;
}
.slick-dots li.slick-active button:before
{
    opacity: .75;
    color: #25a9e0 !important;
}
@media (max-width: 721px) {
  .carouselHeader{
    font-size: 5vw;
    /* margin-bottom: -13%; */
  }
  .carousel{
    height: 60vh;
    overflow: visible;
    /* max-width: 104%;
    overflow-x: hidden; */
  }
  .carouselDiv{
    height: 64vh;
    background-size: cover;
  }
  .carouselText{
    font-size: 4.3vw;
    padding-top: 12%;
    text-align: center !important;
    color: rgb(255,255,255, 0.8);
  }
}
@media (max-width: 481px) {
  .carouselHeader{
    font-size: 8vw;
    /* margin-bottom: -13%; */
  }
  .carouselText{
    padding-top: 15%;
    font-size: 6vw;
  }
}




